extends layout

block content
  .main.container.clearfix
    .row
      .col-md-8.col-md-offset-2
        h1.display-4
          img.avatar.img-circle.hidden-xs-down(src='/images/avatar.png', alt='avatar')
          | #{name}
        h5 Email
        | #{email}
        h5 Points
        | #{points}
        h5 Created
        | #{createdAt}